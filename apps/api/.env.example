# Aadhaar Pulse API - Environment Configuration
# Copy this to .env and fill in your values

# Environment
ENVIRONMENT=development
DEBUG=true

# Data.gov.in API Configuration
# Get your API key from: https://data.gov.in/user
DATA_GOV_API_KEY=your_api_key_here

# Database (optional - for future PostgreSQL integration)
# DATABASE_URL=postgresql://user:password@localhost:5432/aadhaar_pulse

# Cache Settings
CACHE_TTL_SECONDS=300

# Analytics Settings
FORECAST_HORIZON_MONTHS=6
ANOMALY_ZSCORE_THRESHOLD=2.5
